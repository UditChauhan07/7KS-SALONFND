                        <form id="edit-salon-form" method="POST" action="/salon/create">
                            <!-- Basic Layout -->
                            <div class="row">
                                <div class="col-xl">
                                    <div class="card mb-4">
                                        <div class="card-header d-flex justify-content-between align-items-center">
                                            <h5 class="mb-0">Owner Details</h5>
                                            <small class="text-muted float-end">Fill details of Buisness Owner</small>
                                        </div>
                                        <div class="card-body">
                                            <div class="mb-3">
                                                <label class="form-label"
                                                    for="edit-salon-buisness-owner-name"><sup>*</sup>Name</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="basic-icon-default-fullname2" class="input-group-text"><i
                                                            class="bx bx-building"></i></span>
                                                    <input type="text" class="form-control"
                                                        placeholder="Enter New Business Name"
                                                        aria-label="Enter New Business Name" name="name"
                                                        id="edit-salon-buisness-owner-name"
                                                        aria-describedby="basic-icon-default-fullname2"
                                                        value="{{{editData.businessData.salonOwnerName}}}" />
                                                </div>
                                                <span id="edit-salon-buisness-owner-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label"
                                                    for="edit-salon-buisness-owner-email">Email</label>
                                                <div class="input-group input-group-merge">
                                                    <span class="input-group-text"><i class="bx bx-envelope"></i></span>
                                                    <input type="text" id="edit-salon-buisness-owner-email"
                                                        class="form-control" placeholder="john.doe"
                                                        aria-label="john.doe"
                                                        aria-describedby="edit-salon-buisness-owner-email2" name="email"
                                                        autocomplete="new-email"
                                                        value="{{{editData.businessData.salonOwnerEmail}}}" />
                                                    <span id="edit-salon-buisness-owner-email2"
                                                        class="input-group-text">@example.com</span>
                                                </div>
                                                <span id="edit-salon-buisness-owner-email-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="edit-salon-buisness-owner-phone">Phone
                                                    No</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="edit-salon-buisness-owner-phone2"
                                                        class="input-group-text"><i class="bx bx-phone"></i></span>
                                                    <input type="text" id="edit-salon-buisness-owner-phone"
                                                        oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');"
                                                        name="contactNumber" class="form-control phone-mask"
                                                        placeholder="658 799 8941" aria-label="658 799 8941"
                                                        aria-describedby="edit-salon-buisness-owner-phone2"
                                                        value="{{{editData.businessData.salonOwnerPhone}}}" />
                                                </div>
                                                <span id="edit-salon-buisness-owner-phone-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-3"> <span onclick="SubmiteditUserForm();"
                                            class="btn btn-primary">Next
                                            Step</span></div>
                                </div>
                            </div>
                    </div>
                    </form>

                    <div class="tab-pane fade" id="salonUpdationForm" role="tabpanel">
                        <div class="row">
                            <form action="/imgUpload/business-salon" method="post" enctype="multipart/form-data"
