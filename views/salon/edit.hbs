                                                <input type="text" id="edit-salon-buisness-zipCode" name="zipCode"
                                                    oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');"
                                                    class="form-control" placeholder="214562" aria-label="214562"
                                                    aria-describedby="edit-salon-buisness-zipCode2"
                                                    value="{{{editData.businessData.addressfield4}}}" />
                                            </div>
                                            <span id="edit-salon-buisness-zipCode-error" class="text-danger d-none">this
                                                field is
                                                required</span>
                                        </div>
