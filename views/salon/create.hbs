                                            <div class="input-group input-group-merge mt-2">
                                                <div class="form-check form-switch mb-2">
                                                    <input class="form-check-input" type="checkbox" value="1"
                                                        name="isEnabled" id="create-salon-buisness-isEnabled" checked />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="d-flex">
                                            <label for="defaultCheck3" class="form-label">Verified
                                                Buisness</label>
                                            <div class="input-group input-group-merge mt-2">
                                                <div class="form-check form-switch mb-2">
                                                    <input class="form-check-input" type="checkbox" value="1"
                                                        name="isVerifed" id="create-salon-buisness-isisVerifed" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl">
                                <div class="card mb-4">
                                    <div class="card-header d-flex justify-content-between align-items-center">
                                        <h5 class="mb-0">Address Details</h5>
                                        <small class="text-muted float-end">Buisness Address details</small>
                                    </div>
                                    <div class="card-body">

                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-address"><sup>*</sup>Address</label>
                                            <div class="input-group input-group-merge">
                                                <span id="create-salon-buisness-address2" class="input-group-text"><i
                                                        class="bx bx-home"></i></span>
                                                <textarea id="create-salon-buisness-address" class="form-control"
                                                    rows="4" placeholder="304B, Manious Street"
                                                    aria-label="304B, Manious Street" name="address"
                                                    aria-describedby="create-salon-buisness-address2"></textarea>
                                            </div>
                                            <span id="create-salon-buisness-address-error"
                                                class="text-danger d-none">this field is
                                                required</span>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-city"><sup>*</sup>City</label>
                                            <div class="input-group input-group-merge">
                                                <span id="create-salon-buisness-city2" class="input-group-text"><i
                                                        class="bx bx-buildings"></i></span>
                                                <input type="text" class="form-control" id="create-salon-buisness-city"
                                                    placeholder="Madrid" aria-label="Madrid" name="city"
                                                    aria-describedby="create-salon-buisness-city2" />
                                            </div>
                                            <span id="create-salon-buisness-city-error" class="text-danger d-none">this
                                                field is
                                                required</span>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-state"><sup>*</sup>State</label>
                                            <div class="input-group input-group-merge">
                                                <span id="create-salon-buisness-state2" class="input-group-text"><i
                                                        class="bx bx-stats"></i></span>
                                                <input type="text" id="create-salon-buisness-state" name="state"
                                                    class="form-control" placeholder="Barcelona" aria-label="Barcelona"
                                                    aria-describedby="create-salon-buisness-state2" />
                                            </div>
                                            <span id="create-salon-buisness-state-error" class="text-danger d-none">this
                                                field is
                                                required</span>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-zipCode"><sup>*</sup>Zip
                                                Code</label>
                                            <div class="input-group input-group-merge">
                                                <span class="input-group-text"><i class="bx bx-directions"></i></span>
                                                <input type="text" id="create-salon-buisness-zipCode" name="zipCode"
                                                    oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');"
                                                    class="form-control" placeholder="214562" aria-label="214562"
                                                    aria-describedby="create-salon-buisness-zipCode2" />
                                            </div>
                                            <span id="create-salon-buisness-zipCode-error"
                                                class="text-danger d-none">this field is
                                                required</span>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-country"><sup>*</sup>Country</label>
                                            <div class="input-group input-group-merge">
                                                <span class="input-group-text"><i class="bx bx-shape-circle"></i></span>
                                                <select id="create-salon-buisness-country" name="country"
                                                    class="select2 form-select">
                                                    <option value="">Select</option>
                                                    {{#each countries}}
                                                    <option value="{{{id}}}">{{{name}}}</option>
                                                    {{/each}}
                                                </select>
                                            </div>
                                            <span id="create-salon-buisness-country-error"
                                                class="text-danger d-none">this field is
                                                required</span>
                                        </div>
                                        <div class="mb-3">
                                            <label class="form-label"
                                                for="create-salon-buisness-latitute"><sup>*</sup>Latitute</label>
                                            <div class="input-group input-group-merge">
                                                <span class="input-group-text"><i class="bx bx-x"></i></span>
                                                <input type="text" id="create-salon-buisness-latitute" name="latitude"
                                                    oninput="this.value = this.value.replace(/[^0-9.-]/g, '').replace(/(\..*)\./g, '$1');"
                                                    class="form-control" placeholder="30.674" aria-label="30.674"
                                                    aria-describedby="create-salon-buisness-zipCode2" />
                                            </div>
                                            <span id="create-salon-buisness-latitute-error"
                                                class="text-danger d-none">this field is
                                                required</span>
                                        </div>
