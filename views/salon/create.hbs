                        <form id="create-salon-form" method="POST" action="/salon/create">
                            <!-- Basic Layout -->
                            <div class="row">
                                <div class="col-xl">
                                    <div class="card mb-4">
                                        <div class="card-header d-flex justify-content-between align-items-center">
                                            <h5 class="mb-0">Owner Details</h5>
                                            <small class="text-muted float-end">Fill details of Buisness Owner</small>
                                        </div>
                                        <div class="card-body">
                                            <div class="mb-3">
                                                <label class="form-label"
                                                    for="create-salon-buisness-owner-name"><sup>*</sup>Name</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="basic-icon-default-fullname2" class="input-group-text"><i
                                                            class="bx bx-building"></i></span>
                                                    <input type="text" class="form-control" placeholder="john doe"
                                                        aria-label="john doe" name="name"
                                                        id="create-salon-buisness-owner-name"
                                                        aria-describedby="basic-icon-default-fullname2" />
                                                </div>
                                                <span id="create-salon-buisness-owner-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label"
                                                    for="create-salon-buisness-owner-email">Email</label>
                                                <div class="input-group input-group-merge">
                                                    <span class="input-group-text"><i class="bx bx-envelope"></i></span>
                                                    <input type="text" id="create-salon-buisness-owner-email"
                                                        class="form-control" placeholder="john.doe"
                                                        aria-label="john.doe"
                                                        aria-describedby="create-salon-buisness-owner-email2"
                                                        name="email" autocomplete="new-email" />
                                                    <span id="create-salon-buisness-owner-email2"
                                                        class="input-group-text">@example.com</span>
                                                </div>
                                                <span id="create-salon-buisness-owner-email-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="create-salon-buisness-owner-phone">Phone
                                                    No</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="create-salon-buisness-owner-phone2"
                                                        class="input-group-text"><i class="bx bx-phone"></i></span>
                                                    <input type="text" id="create-salon-buisness-owner-phone"
                                                        oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');"
                                                        name="contactNumber" class="form-control phone-mask"
                                                        placeholder="6587998941" aria-label="6587998941"
                                                        aria-describedby="create-salon-buisness-owner-phone2" />
                                                </div>
                                                <span id="create-salon-buisness-owner-phone-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label"
                                                    for="create-salon-buisness-password">Password</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="create-salon-buisness-password2"
                                                        class="input-group-text"><i
                                                            class="bx bx-fingerprint"></i></span>
                                                    <input type="password" id="create-salon-buisness-password"
                                                        name="password" class="form-control"
                                                        aria-describedby="create-salon-buisness-password2"
                                                        placeholder="********" autocomplete="new-password" />
                                                </div>
                                                <div class="form-text">You can use letters, numbers & periods</div>
                                                <span id="create-salon-buisness-password-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="create-salon-buisness-c-password">Confim
                                                    Passwrod</label>
                                                <div class="input-group input-group-merge">
                                                    <span id="create-salon-buisness-c-password2"
                                                        class="input-group-text"><i
                                                            class="bx bx-fingerprint"></i></span>
                                                    <input type="password" id="create-salon-buisness-c-password"
                                                        name="password1" placeholder="********" class="form-control"
                                                        aria-describedby="create-salon-buisness-c-password2" />
                                                </div>
                                                <div class="form-text">You can use letters, numbers & periods</div>
                                                <span id="create-salon-buisness-c-password-error"
                                                    class="text-danger d-none">this field is
                                                    required</span>
                                            </div>
                                            {{!-- <div class="form-check mt-4">
                                                <input class="form-check-input" type="checkbox" value="1"
                                                    name="isEnabled" id="create-salon-buisness-isEnabled" checked />
                                                <label class="form-check-label" for="create-salon-buisness-isEnabled">
                                                    Active this User
                                                </label>
                                            </div> --}}
                                        </div>
                                    </div>
                                    <div class="col-3"> <span onclick="SubmitCreateUserForm();"
                                            class="btn btn-primary">Next&#8594;</span></div>
                                </div>
                            </div>
                    </div>
                    </form>

                    <div class="tab-pane fade" id="salonCreationForm" role="tabpanel">
                        <div class="row">
                            <form action="/imgUpload/business-salon" method="post" enctype="multipart/form-data"
                                id="uploadImgCreatesalon">
                                <div class="card-body">
                                    <div class="d-flex align-items-start align-items-sm-center gap-4">
