                                                <div class="col-md-4">
                                                    <div class="form-check form-switch">

                                                        <input class="form-check-input weekCheck" type="checkbox"
                                                            value="1" name="tueCheck" id="tueCheck" data-week="1"
                                                            checked />
                                                    </div>
                                                </div>
                                            </div>
