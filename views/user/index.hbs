        <div class="card-body">
            <div class="table-responsive text-nowrap">
                {{#if tableStatus.ethnicity}}
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Description</th>
                            <th>Status</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each tableData}}
                        <tr>
                            <td>
                                <i class="fab fa-angular fa-lg text-danger me-3"></i> <strong>{{{name}}}</strong>
                            </td>
                            <td>{{{description}}}</td>
                            <td>{{#if isEnabled}}<span class="badge bg-label-primary me-1">Active </span>{{else}}<span
                                    class="badge bg-label-warning me-1">InActive</span> {{/if}}</td>
                            <td>
                                <div class="dropdown">
                                    <button type="button" class="btn p-0 dropdown-toggle hide-arrow"
                                        data-bs-toggle="dropdown">
                                        <i class="bx bx-dots-vertical-rounded"></i>
                                    </button>
                                    <div class="dropdown-menu">
                                        {{#if data.permission.userChild.edit}}
                                        <a class="dropdown-item" href="/user/{{{id}}}"><i
                                                class="bx bx-edit-alt me-1"></i> Edit</a>
                                        {{/if}}
                                        {{#if data.permission.userChild.delete}}
                                        <a class="dropdown-item" href="/delete/{{{id}}}"><i
                                                class="bx bx-trash me-1"></i> Delete</a>
                                        {{/if}}
                                    </div>
                                </div>
                            </td>
                        </tr>
                        {{else}}
                        <tr>
                            <td>No Data Found.</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
                {{/if}}
                {{#if tableStatus.user}}
                <table class="table table-bordered">
